<h1>Listing pydwgns</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Ward</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pydwgns.each do |pydwgn| %>
  <tr>
    <td><%= pydwgn.name %></td>
    <td><%= pydwgn.value %></td>
    <td><%= pydwgn.latitude %></td>
    <td><%= pydwgn.longitude %></td>
    <td><%= pydwgn.ward %></td>
    <td><%= link_to 'Show', pydwgn %></td>
    <td><%= link_to 'Edit', edit_pydwgn_path(pydwgn) %></td>
    <td><%= link_to 'Destroy', pydwgn, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Pydwgn', new_pydwgn_path %>
